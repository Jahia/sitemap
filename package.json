    "test": "node --max-old-space-size=4096 ./node_modules/jest/bin/jest.js --coverage --logHeapUsage --ci --maxWorkers=50% --silent",
    "tdd": "jest --watch",
    "lint:js": "eslint --ext js,jsx src",
    "lint:js:fix": "yarn lint:js --fix src",
    "lint:scss": "stylelint './src/**/*.scss' --allow-empty-input",
    "lint:scss:fix": "yarn lint:scss --fix",
